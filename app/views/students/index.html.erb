<div class="header-container <%=@color_scheme%>_header">
<h1 class="text-center <%=@color_scheme%>-h">imma study</h1></div>

<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
      </tr>
    </thead>
    <tbody>
      <% @students.each do |student| %>
      <tr>
        <td> <a href="/students/<%=student.id%>"><%= student.first_name %></a></td>
        <td><%= student.last_name %></td>
        <td><%= student.age %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
