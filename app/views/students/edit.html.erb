<%= form_for @student do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name, value: @student.first_name%>
    <%= f.label :last_name %>
    <%= f.text_field :last_name, value: @student.last_name%>
    <%= f.label :age %>
    <%= f.number_field :age, value: @student.age%>
    <%= f.label :education %>
    <%= f.text_field :education, value: @student.education%>
    <%= f.submit %>
<% end %>

<%= link_to 'Expel', student_path(@student), method: :delete %>
