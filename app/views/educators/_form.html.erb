<%= form_for @educator, remote: true do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name%>
<%= f.label :last_name %>
<%= f.text_field :last_name%>
<%= f.label :age %>
<%= f.number_field :age%>
<%= f.label :salary %>
<%= f.number_field :salary%>
<%= f.label :education %>
<%= f.text_field :education%>
<%= f.collection_select :house_id, House.order(:name),:id,:name %>
<input type="email" name="email"></input>

<%= f.submit %>
<% end %>
