<%= bootstrap_form_for @educator do |f| %>

<div class="form-row">
  <div class="col">
<%= f.text_field :first_name%>
</div>
  <div class="col">
<%= f.text_field :last_name%>
</div>
</div>
<div class="form-row">
  <div class="col">
<%= f.number_field :age%>
</div>
  <div class="col">
<%= f.number_field :salary%>
</div>
</div>
<div class="form-row">

  <div class="col">
<%= f.select :education, ['High School',  'College' , 'Masters',  'Phd']%>
</div>
  <div class="col">
<%= f.collection_select :house_id, House.order(:name),:id,:name %>
</div>
</div>
    <%= f.submit %>
<% end %>
