<%= form_for @cohort, remote:true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :start %>
  <%= f.date_field :start%>
  <%= f.label :end %>
  <%= f.date_field :end%>
  <%= f.label :educator_id %>
  <%= f.collection_select :educator_id, Educator.order(:last_name), :id,:last_name, include_blank: true %>
  <%= f.hidden_field :course_id, value: params[:course_id]%>

  <%= f.submit %>
<% end %>
