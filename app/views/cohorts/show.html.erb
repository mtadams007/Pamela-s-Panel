<div class="header-container <%=@color_scheme%>_header">
<h1 class="text-center <%=@color_scheme%>_h"><%= @cohort.name %> </h1>
</div>
<h2>From:<%= @cohort.start %> Until:<%= @cohort.end %> </h2>
<h2> Taught by: Professor <%= @cohort.educator.last_name %> </h2>

<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>

      </tr>
    </thead>
    <tbody id="cohort_students">
      <% @cohort.students.each do |student| %>
      <tr>
        <td> <%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.age %></td>

      </tr>
      <% end %>
    </tbody>
  </table>
</div>
